@media (-webkit-min-device-pixel-ratio: 3), (-min-device-pixel-ratio: 3) {
}
body,
html {
  line-height: 1;
  font-weight: 200;
  font-family: 'PingFang SC', 'STHeitiSC-Light', 'Helvetica-Light', 'arial', 'sans-serif';
}
@media (-webkit-min-device-pixel-ratio: 1.5), (-min-device-pixel-ratio: 1.5) {
  .border-1px::before,
  .grid-cell::before {
    -webkit-transform: scaleX(0.7);
    transform: scaleX(0.7);
  }
  .border-1px::after,
  .grid-cell::after {
    -webkit-transform: scaleY(0.7);
    transform: scaleY(0.7);
  }
}
@media (-webkit-min-device-pixel-ratio: 2), (-min-device-pixel-ratio: 2) {
  .border-1px::before,
  .grid-cell::before {
    -webkit-transform: scaleX(0.5);
    transform: scaleX(0.5);
  }
  .border-1px::after,
  .grid-cell::after {
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
  }
}
.clear {
  clear: both;
}
.clearfix {
  display: inline-block;
}
.clearfix:after {
  display: block;
  content: ".";
  height: 0;
  line-height: 0;
  clear: both;
  visibility: hidden;
}
a {
  text-decoration: none;
}
.header {
  height: 44px;
  line-height: 44px;
  font-size: 18px;
  font-weight: 400;
  padding: 0 20px;
  background-color: #7caf49;
  color: #fff;
}
.header .left {
  float: left;
}
.header .right {
  float: right;
}
.wrapper {
  width: auto;
  height: auto;
  padding: 0 15px;
  padding-bottom: 100px;
  background-color: #ebebeb;
}
.wrapper .user-info {
  height: auto;
  padding: 10px;
  padding-bottom: 0;
  background-color: #fff;
  border-radius: 0 0 5px 8px;
}
.wrapper .user-info .base-info {
  border-bottom: 1px solid #ccc;
  padding-bottom: 10px;
}
.wrapper .user-info .base-info .username {
  font-size: 26px;
  font-weight: 300;
  height: 32px;
  line-height: 32px;
  color: #333;
}
.wrapper .user-info .base-info .tags {
  height: auto;
  line-height: 26px;
  margin-top: 6px;
}
.wrapper .user-info .base-info .tags .tag {
  display: inline-block;
  float: left;
  height: 18px;
  line-height: 18px;
  padding: 0 10px;
  margin-right: 8px;
  border: 1px solid #999;
  font-size: 12px;
  font-weight: 200;
  color: #999;
}
.wrapper .user-info .contact-info {
  padding: 10px;
}
.wrapper .user-info .contact-info p {
  height: 18px;
  line-height: 18px;
  font-size: 14px;
  font-weight: 300;
  color: #999;
}
.wrapper .history-list {
  list-style: none;
  height: auto;
  margin-top: 43px;
}
.wrapper .history-list li {
  position: relative;
  padding-bottom: 43px;
  list-style: none;
  height: auto;
  border-left: 1px solid #7caf49;
}
.wrapper .history-list li .time-line {
  display: inline-block;
  position: relative;
  height: 28px;
  line-height: 28px;
  width: auto;
  top: -14px;
  left: 16px;
  padding: 0 20px;
  border-radius: 28px;
  font-size: 14px;
  font-weight: 300;
  background-color: #7caf49;
  color: #fff;
}
.wrapper .history-list li .time-line::before {
  content: " ";
  display: block;
  position: absolute;
  left: -16px;
  top: 13.5px;
  width: 16px;
  border-bottom: 1px solid #7caf49;
}
.wrapper .history-list li .time-line::after {
  content: " ";
  display: block;
  position: absolute;
  width: 15px;
  height: 15px;
  left: -23.5px;
  top: 6.5px;
  border-radius: 50%;
  background: radial-gradient(#7caf49, #fff);
  background: -webkit-radial-gradient(#7caf49, #fff);
}
.wrapper .history-list li .msg-box {
  position: relative;
  width: auto;
  height: auto;
  margin: 0 36px;
  margin-top: 20px;
  margin-right: 16px;
  padding: 10px;
  padding-left: 20px;
  border-radius: 5px;
  background-color: #fff;
}
.wrapper .history-list li .msg-box .avatar {
  display: block;
  position: absolute;
  left: -20px;
  top: -20px;
  width: 40px;
  height: 40px;
  border-radius: 50%;
  background-image: url("../images/icon/zuoxi-icon@2x.png");
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center center;
  background-color: #c9d8b9;
}
.wrapper .history-list li .msg-box .content {
  height: auto;
  line-height: 22px;
  font-size: 14px;
  font-weight: 300;
  color: #555;
}
.wrapper .history-list li .msg-box.saleManager-msg {
  background-color: #dae3eb;
}
.wrapper .history-list li .msg-box.saleManager-msg .avatar {
  background-image: url("../images/icon/xiaoshou-icon@2x.png");
  background-color: #d0deec;
}
