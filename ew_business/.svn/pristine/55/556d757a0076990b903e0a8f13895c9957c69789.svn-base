@media (-webkit-min-device-pixel-ratio: 3), (-min-device-pixel-ratio: 3) {
}
body,
html {
  line-height: 1;
  font-weight: 200;
  font-family: 'PingFang SC', 'STHeitiSC-Light', 'Helvetica-Light', 'arial', 'sans-serif';
}
@media (-webkit-min-device-pixel-ratio: 1.5), (-min-device-pixel-ratio: 1.5) {
  .border-1px::before,
  .grid-cell::before {
    -webkit-transform: scaleX(0.7);
    transform: scaleX(0.7);
  }
  .border-1px::after,
  .grid-cell::after {
    -webkit-transform: scaleY(0.7);
    transform: scaleY(0.7);
  }
}
@media (-webkit-min-device-pixel-ratio: 2), (-min-device-pixel-ratio: 2) {
  .border-1px::before,
  .grid-cell::before {
    -webkit-transform: scaleX(0.5);
    transform: scaleX(0.5);
  }
  .border-1px::after,
  .grid-cell::after {
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
  }
}
.clear {
  clear: both;
}
.clearfix {
  display: inline-block;
}
.clearfix:after {
  display: block;
  content: ".";
  height: 0;
  line-height: 0;
  clear: both;
  visibility: hidden;
}
a {
  text-decoration: none;
}
body {
  background-color: #e8e8e8;
}
.calendar-header {
  height: auto;
  padding: 12px;
  padding-top: 14px;
  background-color: #7caf49;
  color: #fff;
}
.calendar-header h1 {
  font-size: 24px;
  font-weight: 400;
  height: 28px;
  line-height: 28px;
}
.calendar-header p {
  font-size: 14px;
  font-weight: 200px;
  height: 16px;
  line-height: 16px;
  color: #ccc;
}
.wrapper {
  width: auto;
  height: auto;
  margin: 12px;
  margin-top: 0;
  border-radius: 0 0 8px 8px;
  background-color: #fff;
}
.wrapper .weekend-picker {
  width: 100%;
  height: auto;
}
.wrapper .weekend-picker tr td {
  text-align: center;
}
.wrapper .weekend-picker tr td a {
  display: inline-block;
  height: 32px;
  line-height: 32px;
  width: 32px;
  color: #999;
  font-size: 18px;
  font-weight: 500;
}
.wrapper .weekend-picker tr td.active a,
.wrapper .weekend-picker tr td a.active {
  border-radius: 50%;
  background-color: #7caf49;
  color: #fff;
}
.wrapper .list {
  list-style: none;
}
.wrapper .list li {
  list-style: none;
}
.wrapper .list .time-line {
  height: 28px;
  line-height: 28px;
  margin: 5px 10px;
  margin-bottom: 0;
  border-bottom: 1px solid #999;
  opacity: 0.3;
  color: #7caf49;
  font-weight: 400;
  font-size: 18px;
}
.wrapper .list .item {
  position: relative;
  width: auto;
  height: auto;
  padding: 0 15px;
}
.wrapper .list .item .content {
  position: relative;
  width: auto;
  height: auto;
  padding: 15px 0;
  padding-left: 25px;
  border-bottom: 1px solid #ccc;
  background-image: url("../images/icon/biaoqian-icon@2x.png");
  background-repeat: no-repeat;
  background-size: 23px auto;
  background-position: 2px 10px;
}
.wrapper .list .item .content::before {
  content: " ";
  display: block;
  position: absolute;
  top: 15px;
  left: 0;
  width: 17px;
  height: 40px;
  height: auto;
  background-image: url("../images/icon/biaoqian-icon@2x.png");
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center top;
}
.wrapper .list .item .content .title {
  height: 28px;
  line-height: 28px;
  margin-top: -5px;
  padding-right: 15px;
  width: auto;
  color: #777;
  font-size: 22px;
  font-weight: 400;
}
.wrapper .list .item .content .title .time {
  font-weight: 400;
  color: #444;
  float: right;
}
.wrapper .list .item .content .name {
  height: 20px;
  line-height: 20px;
  width: auto;
  color: #999;
  font-size: 18px;
  font-weight: 200;
}
.wrapper .list .item .content .tags {
  height: auto;
  line-height: 24px;
  margin-top: 0;
}
.wrapper .list .item .content .tags::after {
  content: " ";
  display: block;
  clear: both;
}
.wrapper .list .item .content .tags .tag {
  display: inline-block;
  float: left;
  height: 14px;
  line-height: 14px;
  padding: 0 10px;
  margin-right: 8px;
  border: 1px solid #999;
  font-size: 10px;
  font-weight: 200;
  color: #999;
}
.wrapper .list .item:last-child .content {
  border-bottom: none;
}
