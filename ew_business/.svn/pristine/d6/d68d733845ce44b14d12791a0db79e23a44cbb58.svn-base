<script type="text/javascript">
$(document).ready(function(){
<#if (Session.user.id)??>
	<#-- 初始化当前页面的JS事件 -->
	$.Utils.initPageEvent();
	$("#svc-switcher").on("change", function(){
		location.href = "${ctx.contextPath}/?svc="+$(this).val();
	});
	$.Utils.focusMenu();
	$.Utils.startTicker();
	<#-- 显示提示信息 -->
	<#if result??>
		<#if result[0] == 'true'>
			toastr.success('${result[1]}', "操作成功");		
		<#else>
			toastr.error('${result[1]}', "操作失败");
		</#if>
	</#if>
	<#if errors??>
		toastr.error("${errors}", "数据校验未通过");
	</#if>
	$("select.select2").select2();
</#if>
});
</script>